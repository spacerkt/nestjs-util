{
  "name": "spacerkt-util-nestjs",
  "version": "0.0.25",
  "description": "Common utilities for NestJS",
  "author": "Space Rocket",
  "license": "MIT",
  "url": "https://github.com/spacerkt/nestjs-util#readme",
  "scripts": {
    "prepare": "npm run build",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.6.5",
    "@nestjs/microservices": "^7.6.5",
    "@nestjs/swagger": "^4.7.9",
    "@nestjs/testing": "^7.6.5",
    "@nestjs/typeorm": "^7.1.5",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.13.39",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.29",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.7.0 || ^7.0.0",
    "@nestjs/core": "^6.7.0 || ^7.0.0",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/swagger": "^4.5.7",
    "typeorm": "^0.2.24",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spacerkt/nestjs-util"
  }
}
